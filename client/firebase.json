{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "public", // Folder containing your frontend files
    "rewrites": [
      {
        "source": "/sendRentReminder",
        "function": "sendRentReminder"
      }
    ]
  }
}